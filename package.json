{
  "name": "validator-maker",
  "version": "0.1.2",
  "description": "Create complex validators from simple ones",
  "keywords": [
    "validator"
  ],
  "author": "Hodoug Joung",
  "repository": {
    "type": "git",
    "url": "https://github.com/anisotropy/validator-maker"
  },
  "homepage": "https://github.com/anisotropy/validator-maker",
  "type": "module",
  "main": "./dist/validator-maker.umd.cjs",
