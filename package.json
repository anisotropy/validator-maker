    "eslint": "^8.52.0",
    "eslint-plugin-stratified-design": "^0.9.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3",
    "vitest": "^0.34.6"
